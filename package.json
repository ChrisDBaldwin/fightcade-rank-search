{
  "name": "fc-rank-search",
  "version": "1.0.0",
  "description": "A tool for finding accurate player rank from table of ranked players on Fightcade",
  "main": "dist/server.js",
  "scripts": {
    "build": "tsc",
    "start": "node -r ./instrumentation.js dist/server.js",
    "dev": "NODE_OPTIONS='-r ./instrumentation.js' ts-node-dev --respawn --transpile-only src/server.ts",
    "fetch-data": "ts-node src/scripts/fetchRankings.ts",
    "type-check": "tsc --noEmit"
  },
  "keywords": ["fightcade", "ranking", "search", "gaming"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@opentelemetry/api": "^1.8.0",
    "@opentelemetry/auto-instrumentations-node": "^0.43.0",
    "@opentelemetry/exporter-metrics-otlp-http": "^0.49.1",
    "@opentelemetry/exporter-trace-otlp-http": "^0.49.1",
    "@opentelemetry/instrumentation": "^0.49.1",
    "@opentelemetry/instrumentation-express": "^0.38.0",
    "@opentelemetry/instrumentation-http": "^0.49.1",
    "@opentelemetry/resources": "^1.22.0",
    "@opentelemetry/sdk-metrics": "^1.22.0",
    "@opentelemetry/sdk-node": "^0.49.1",
    "@opentelemetry/sdk-trace-base": "^1.22.0",
    "@opentelemetry/sdk-trace-node": "^1.22.0",
    "@opentelemetry/semantic-conventions": "^1.22.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "fightcade-api": "^1.0.0",
    "helmet": "^7.1.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^20.10.0",
    "@types/cors": "^2.8.17",
    "@types/compression": "^1.7.5",
    "typescript": "^5.3.2",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0"
  }
}
